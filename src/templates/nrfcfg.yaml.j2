configuration:
  MongoDBName: {{ database_name }}
  MongoDBUrl: {{ database_url }}
  mongoDBStreamEnable: true
  nfKeepAliveTime: 60
  nfProfileExpiryEnable: true
  webuiUri: {{ webui_url }}
  sbi:
    bindingIPv4: 0.0.0.0
    port: {{ nrf_sbi_port }}
    registerIPv4: {{ nrf_ip }}
    scheme: {{ scheme }}
    tls:
      pem: {{ tls_pem }}
      key: {{ tls_key }}
  serviceNameList:
  - nnrf-nfm
  - nnrf-disc
info:
  description: NRF initial local configuration
  version: 1.0.0
logger:
  NRF:
    debugLevel: {{ log_level }}
